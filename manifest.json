{
    "manifest_version": 3,
    "name": "Phira Web Chart Downloader (PWCD)",
    "version": "1.1",
    "description": "Download charts from Phira Website (phira.moe)",
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": ["https://phira.moe/*"],
            "js": ["scripts/content.js"]
        }
    ],
    "icons": {
        "16": "res/icon_16x.png",
        "48": "res/icon_48x.png",
        "128": "res/icon_128x.png"
    }
}
